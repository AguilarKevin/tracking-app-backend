{
  "name": "express-todo-app",
  "type": "module",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "private": true,
  "packageManager": "pnpm@7.1.9",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "dev:watch": "nodemon --watch src -e \"js,ts,json\" --delay 1.5 --exec \"pnpm dev\"",
    "dev": "ts-node-esm --project ./tsconfig.build.json src",
    "lint:all": "pnpm run lint && pnpm run lint:ts",
    "lint:fix": "pnpm run lint --fix",
    "lint:ts": "pnpm run build --noEmit",
    "lint": "eslint --ext .js,.ts src",
    "start": "node dist/index.js",
    "test": "tap --node-arg=--loader=ts-node/esm --test-regex=\".(spec|test).([mc]js|[jt]sx?)$\" --no-coverage ",
    "test:watch": "tap --watch",
    "test:cov": "tap"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@typegoose/typegoose": "^9.11.0",
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "mongoose": "^6.5.2",
    "morgan": "^1.10.0",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/chai": "^4.3.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.11.49",
    "@types/tap": "^15.0.7",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "chai": "^4.3.6",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-xo": "^0.40.0",
    "eslint-config-xo-typescript": "^0.50.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-security": "^1.5.0",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "prisma": "^4.2.1",
    "tap": "^16.3.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
